# Stop the configuration file's bubbling behavior to avoid inheriting this configuration in subdirectories or submodules
config.stopBubbling=true

# When generating the toString method, call the superclass's toString method to include field information from the parent class
lombok.tostring.callsuper=CALL

# When generating equals and hashCode methods, call the superclass's versions to ensure inherited comparison logic is included
lombok.equalsandhashcode.callsuper=CALL

# Enable accessor chaining, allowing multiple setter methods to be called consecutively for better readability and conciseness
lombok.accessors.chain=true
